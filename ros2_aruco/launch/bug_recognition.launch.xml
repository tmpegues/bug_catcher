<launch>

    <include file="$(find-pkg-share realsense2_camera)/launch/rs_launch.py">
        <arg name="depth_module.profile" value="1280x720x30"/>
        <arg name="pointcloud.enable" value="true"/>
        <arg name="rgb_camera.profile" value="1280x720x30"/>
    </include>

    <include file="$(find-pkg-share ros2_aruco)/launch/aruco_recognition.launch.py"/>

    <!-- RViz -->
    <node pkg="rviz2" exec="rviz2" args="-d $(find-pkg-share ros2_aruco)/config/camera_rviz.rviz"/>
</launch>